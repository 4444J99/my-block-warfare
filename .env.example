# TurfSynth AR Backend Configuration

# Database
DATABASE_URL=postgresql://localhost:5432/turfsynth
DATABASE_POOL_SIZE=20

# Redis
REDIS_URL=redis://localhost:6379
REDIS_CLUSTER_MODE=false

# Server
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# H3 Configuration
H3_RESOLUTION_STORAGE=7
H3_RESOLUTION_GAMEPLAY=9

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# Geofencing
SPEED_LOCKOUT_KMH=15
SPEED_WINDOW_SECONDS=30
SPOOF_VELOCITY_MAX_KMH=500

# Zone Data Sources (optional)
OSM_API_URL=https://overpass-api.de/api/interpreter
SAFEGRAPH_API_KEY=

# Logging
LOG_LEVEL=info
